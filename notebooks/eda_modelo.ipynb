    "# Função de limpeza de texto\n",
    "def limpar_texto(texto):\n",
    "    if not isinstance(texto, str):\n",
    "        return \"\"\n",
    "    texto = texto.lower()\n",
    "    texto = re.sub(f'[{re.escape(string.punctuation)}]', '', texto)\n",
    "    texto = re.sub(r'\\d+', '', texto)\n",
    "    tokens = word_tokenize(texto)\n",
    "    stop_words = set(stopwords.words('portuguese'))\n",
    "    tokens_filtrados = [palavra for palavra in tokens if palavra not in stop_words]\n",
    "    return ' '.join(tokens_filtrados)\n",
    "\n",
    "# Selecionando colunas de texto relevantes da estrutura do CSV\n",
    "colunas_texto_vaga = ['titulo_vaga_detalhado', 'atividades_vaga', 'competencias_vaga', 'areas_atuacao_vaga']\n",
    "colunas_texto_candidato = ['cv_texto_pt', 'applicant_area_atuacao', 'applicant_conhecimentos']\n",
    "\n",
    "# Combinando todas as colunas de texto em 'soup' e aplicando a limpeza\n",
    "# Usamos fillna('') para garantir que valores nulos não causem erro\n",
    "df_final['soup_vaga'] = df_final[colunas_texto_vaga].fillna('').agg(' '.join, axis=1).apply(limpar_texto)\n",
    "df_final['soup_candidato'] = df_final[colunas_texto_candidato].fillna('').agg(' '.join, axis=1).apply(limpar_texto)\n",
    "\n",
    "print(\"Colunas 'soup_vaga' e 'soup_candidato' criadas e limpas.\")\n",
    "\n",
    "# Visualizando o resultado final\n",
    "df_final[['situacao_candidado', 'candidato_aderente', 'soup_vaga', 'soup_candidato']].head()"
   ]
